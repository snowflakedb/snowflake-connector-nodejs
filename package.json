    "expand-tilde": "^2.0.2",
    "fast-xml-parser": "^4.2.5",
    "fastest-levenshtein": "^1.0.16",
    "generic-pool": "^3.8.2",
    "glob": "^10.0.0",
    "https-proxy-agent": "^7.0.2",
    "jsonwebtoken": "^9.0.0",
    "mime-types": "^2.1.29",
